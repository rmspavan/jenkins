pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Build the code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy the App'
            }
        }
        stage('Test') {
            steps {
                echo 'Test the App'
            }
        }
        stage('Release') {
            steps {
                echo 'Release the App to Production'
            }
        }
    }
}
